{
  "name": "chattur-server",
  "description": "Chattur React App and Express Backend",
  "version": "1.0.0",
  "scripts": {
    "start": "npm run prod",
    "server": "babel-node ./src/server.js",
    "server:prod": "node ./dist/server.js",
    "build": "babel ./src --out-dir dist/ --ignore ./node_modules,./.babelrc,./package.json,./npm-debug.log --copy-files",
    "clean": "rimraf dist/",
    "dev": "SET NODE_ENV=development && npm run server",
    "prod": "SET NODE_ENV=production && npm run build && npm run server:prod",
    "watch": "nodemon"
  },
  "dependencies": {
    "async": "^3.0.1",
    "axios": "0.18.0",
    "body-parser": "^1.18.3",
    "co": "^4.6.0",
    "express": "^4.16.4",
    "mongodb": "^3.2.6",
    "mongoose": "*",
    "morgan": "^1.9.1",
    "tmp-promise": "2.0.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "nodemon": "^1.19.0",
    "rimraf": "^2.6.3",
    "babel-watch": "7.0.0"
  }
} 
